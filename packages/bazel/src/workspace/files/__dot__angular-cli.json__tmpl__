{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "<%= fileName %>"
  },
  "apps": [
  ],
  "test": {
    "karma": {
      "config": "node_modules/@nxpkg/bazel/src/utils/karma.conf.js"
    }
  },
  "defaults": {
    "schematics": {
      "collection": "@nxpkg/bazel"
    },
    "component": {
      "spec": true,
      "inlineStyle": false,
      "inlineTemplate": false
    },
    "directive": {
      "spec": true
    },
    "class": {
      "spec": true
    },
    "guard": {
      "spec": true
    },
    "module": {
      "spec": true
    },
    "pipe": {
      "spec": true
    },
    "service": {
      "spec": true
    }
  }
}
